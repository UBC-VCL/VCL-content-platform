{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@adapters": ["src/adapters"],
      "@adapters/*": ["src/adapters/*"],
      "@redux": ["src/redux"],
      "@redux/*": ["src/redux/*"],
      "@components": ["src/components"],
      "@components/*": ["src/components/*"],
      "@pages": ["src/pages"],
      "@pages/*": ["src/pages/*"],
      "@statics": ["src/statics"],
      "@statics/*": ["src/statics/*"]
    }
  }
}
